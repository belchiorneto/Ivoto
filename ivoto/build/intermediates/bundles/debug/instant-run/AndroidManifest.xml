<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lojapro.can"
    android:versionCode="22"
    android:versionName="1.21" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="21" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <permission
        android:name="com.example.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.example.permission.MAPS_RECEIVE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <!--
    <uses-permission android:name="android.permission.READ_PROFILE"/>
	<uses-permission android:name="android.permission.READ_CONTACTS"/>

	<uses-permission android:name="android.permission.GET_ACCOUNTS" />
    -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <uses-library android:name="com.google.android.maps" />
        <!-- API key for the Android Maps API v2. The value is defined as a string resource. -->

        <receiver
            android:name="com.google.android.gms.analytics.AnalyticsReceiver"
            android:enabled="true" >
            <intent-filter>
                <action android:name="com.google.android.gms.analytics.ANALYTICS_DISPATCH" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.google.android.gms.analytics.AnalyticsService"
            android:enabled="true"
            android:exported="false" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAFk8rFvKU-nzyleEVkCaC7WrijWdHnfaI" />
        <meta-data
            android:name="android.app.searchable"
            android:resource="@xml/searchable" />
        <!-- Classes de teste do sistema de notificação -->
        <activity
            android:name="com.lojapro.can.NotificationOne"
            android:label="notifica1"
            android:launchMode="standard" />
        <activity
            android:name="com.lojapro.can.NotificationTwo"
            android:label="notifica2"
            android:launchMode="standard" />

        <!-- Fim Classes de teste do sistema de notificação -->
        <activity
            android:name="com.lojapro.candidato.ComRegistros"
            android:label="@string/existente"
            android:launchMode="standard" />
        <activity
            android:name="com.lojapro.can.Inicio"
            android:label="@string/inicioescolha"
            android:launchMode="standard"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.lojapro.can.ActContato"
            android:label="@string/contatocan"
            android:launchMode="standard" />
        <activity
            android:name="com.lojapro.can.Eleitor"
            android:label="@string/eleitor"
            android:launchMode="standard"
            android:theme="@style/Theme.AppCompat.Light"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.lojapro.candidato.Candidato"
            android:label="@string/candidato"
            android:launchMode="standard"
            android:theme="@style/Theme.AppCompat.Light"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.lojapro.candidato.SemRegistros"
            android:label="@string/precandidato"
            android:launchMode="standard" />
        <activity
            android:name="com.lojapro.candidato.UploadActivity"
            android:label="@string/candidatos"
            android:launchMode="standard" />
        <activity
            android:name="com.lojapro.candidato.RegistrarComoCandidato"
            android:label="@string/candidatos"
            android:launchMode="standard" />
        <activity
            android:name="com.lojapro.can.ListaCidades"
            android:label="@string/app_listacid"
            android:launchMode="standard" />
        <activity
            android:name="com.lojapro.can.ListaCandidatos"
            android:label="@string/app_listacan" />
        <activity
            android:name="com.lojapro.can.CollectionDemoActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.lojapro.can.ActSigle"
            android:label="@string/app_name" />
        <activity
            android:name="com.lojapro.can.Swipe"
            android:label="@string/app_name" />
        <activity
            android:name="com.lojapro.can.ConfiguraRede"
            android:label="configura rede" >
            <intent-filter>
                <action android:name="android.intent.action.MANAGE_NETWORK_USAGE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <!-- Splash screen -->

        <activity
            android:name="com.lojapro.can.SplashScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Main activity -->
        <activity
            android:name="com.lojapro.can.Principal"
            android:label="@string/app_name" >
        </activity>

        <receiver android:name="com.lojapro.alertas.SampleAlarmReceiver" />
        <receiver
            android:name="com.lojapro.alertas.SampleBootReceiver"
            android:enabled="true" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <service android:name="com.lojapro.alertas.SampleSchedulingService" />
        <service android:name="com.lojapro.alertas.RegistraClique" />

        <activity
            android:name="com.google.android.gms.common.api.GoogleApiActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
    </application>

</manifest>